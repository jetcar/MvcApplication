@model System.Collections.Generic.IList<global::MvcApplication.Models.ParkingTimeInfo>

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Parking Start Date")
        </th>
        <th>
            @Html.DisplayName("Parking End Date")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StartTime.TimeOfDay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndTime.TimeOfDay)
            </td>
           
        </tr>
    }

</table>
