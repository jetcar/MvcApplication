@model System.Collections.Generic.List<global::MvcApplication.Models.InvoiceModel>


@{
    ViewBag.Title = "InputData";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@Html.ActionLink("create invoice","CreateInvoice")


<table class="table">


    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink("Invoice detailx","InvoiceDetails",new { id = item.Id })
            </td>
            <td>
                @Html.Display(item.Price.ToString())
            </td>

        </tr>
    }

</table>
